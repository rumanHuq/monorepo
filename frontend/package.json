{
  "name": "@ruman-blog/frontend",
  "version": "1.0.0",
  "scripts": {
    "storybook": "kill-port 9002 && cross-env NODE_ENV=development SHOW_BUNDLE=true TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" start-storybook -p 9002 -c .storybook -s ./",
    "build": "cross-env NODE_ENV=production TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack",
    "analyze": "cross-env NODE_ENV=production SHOW_BUNDLE=true TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack",
    "start": "cross-env NODE_ENV=development TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack-dev-server",
    "test:unit:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --no-cache --runInBand",
    "css": "stylelint '**/*.scss' --syntax scss --config .stylelintrc --fix && stylelint '**/*.vue' --syntax scss --config .vuestylelintrc --fix && :",
    "ts:typecheck": "wotan -c .wotanrc.yaml --fix && :",
    "js": "eslint --config .eslintrc --ext .js,.vue,.ts --max-warnings=500 src --fix && :",
    "test": "jest",
    "clear_jest": "jest --clearCache",
    "lint:all": "yarn css && yarn ts:lint && yarn ts:typecheck && yarn js",
    "import:icon": "gulp icon-font && stylelint 'src/assets/styles/_icons.scss' --syntax scss --config .stylelintrc --fix && stylelint 'src/assets/styles/_icons.scss' --syntax scss --config .stylelintrc --fix",
    "precommit": "lint-staged",
    "prepare": "echo 'gulpifying....' && gulp && echo 'bundling vue....' && yarn prod"
  },
  "author": "",
  "license": "ISC",
