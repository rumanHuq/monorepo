{
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": "..",
    "jsx": "preserve",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "noEmit": true,
    "paths": {
      ".storybook/*": [
        "./frontend/.storybook/*"
      ],
      "@/*": [
        "./frontend/src/*"
      ]
    },
    "target": "ES5"
  },
  "exclude": [
    "node_modules"
  ],
  "extends": "../tsconfig",
  "include": [
    ".eslintrc.js",
    "next-env.d.ts",
    "*.js",
    "**/**/*.ts",
    "**/**/*.tsx"
  ]
}
