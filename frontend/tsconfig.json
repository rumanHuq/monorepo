{
  "compilerOptions": {
    "baseUrl": "./",
    "jsx": "preserve",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "noEmit": true,
    "paths": {
      ".storybook/*": [
        "./.storybook/*"
      ],
      "components/*": [
        "./components/*"
      ],
      "Interfaces/*": [
        "./Interfaces/*"
      ],
      "pages/*": [
        "./pages/*"
      ],
      "styles/*": [
        "./styles/*"
      ],
      "utils/*": [
        "./utils/*"
      ]
    },
    "target": "es5",
    "allowJs": true,
    "module": "esnext"
  },
  "exclude": [
    "node_modules"
  ],
  "extends": "../tsconfig",
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ]
}
